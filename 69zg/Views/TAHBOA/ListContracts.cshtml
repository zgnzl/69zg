@model IEnumerable<_69zg.Models.合同记录>

@{Layout = null;}
<p>
    <a href="javascript:void(0)" onclick="linkliselelct('AddC_contract')">新建合同</a>
</p>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.合同编号)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.甲方)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.乙方)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.签订地点)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.签订时间)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.合同生效日期)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.合同终止日期)
        </th>
      
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.合同编号)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.甲方)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.乙方)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.签订地点)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.签订时间)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.合同生效日期)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.合同终止日期)
        </td>
        <td>
            <a href="javascript:void(0)" onclick="ContractOpt('@item.id','TAHBOA','AlertContract')">编辑</a> |
            <a href="javascript:void(0)" onclick="ContractOpt('@item.id','TAHBOA','DetailContracts')">详细</a> |
            <a href="javascript:void(0)" onclick="ContractOpt('@item.id','TAHBOA','DelContracts','@item.合同编号')">删除</a>
        </td>
    </tr>
}

</table>
